  <PartInfo>
    <Name>custom_anim_LIGHT_BEACON_04</Name>
    <AnimLength>50</AnimLength>
    <Animation>
      <Parameter>
        <Code>
          (A:LIGHT BEACON, bool) if{ (A:TRAILING EDGE FLAPS LEFT PERCENT, percent) 1 &lt;
                                       if{ (A:LIGHT STROBE, bool) 1 ==
					    if{ 0 2250 (A:PLANE ALT ABOVE GROUND, feet) rng
						if{ 35 }
					       els{ 2250 3150 (A:PLANE ALT ABOVE GROUND, feet) rng
							if{ 20 }
						       els{ 0 }

					   els{ (A:PLANE ALT ABOVE GROUND, feet) 4750 &lt;
                                                if{ (A:TRAILING EDGE FLAPS LEFT PERCENT, percent) }
                                               els{ 0 } } } } } }

                                els{ 50 }
        </Code>
        <Lag>20</Lag>
      </Parameter>
    </Animation>
  </PartInfo>
